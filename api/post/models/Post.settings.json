{
  "connection": "default",
  "collectionName": "post",
  "info": {
    "name": "post",
    "description": ""
  },
  "options": {
    "timestamps": [
      "createdAt",
      "updatedAt"
    ]
  },
  "attributes": {
    "title": {
      "configurable": false,
      "unique": true,
      "required": true,
      "type": "string"
    },
    "content": {
      "configurable": false,
      "required": true,
      "type": "richtext"
    },
    "slug": {
      "configurable": false,
      "type": "string"
    },
    "isDraft": {
      "configurable": false,
      "default": true,
      "type": "boolean"
    },
    "cover": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": true
    },
    "description": {
      "required": true,
      "type": "text"
    },
    "date": {
      "configurable": false,
      "required": true,
      "type": "date"
    },
    "hashtags": {
      "configurable": false,
      "required": true,
      "collection": "hashtag",
      "via": "posts",
      "dominant": true
    }
  }
}
